{
	"manifest_version": 3,
	"name": "ThinBridge",
	"version": "2.0.2",
	"description": "ThinBridge for Chrome",
	"permissions": [
		"nativeMessaging",
		"alarms",
		"tabs",
		"webNavigation",
		"webRequest",
		"webRequestBlocking"
	],
	"host_permissions": [
		"<all_urls>"
	],
	"background": {
		"service_worker": "background.js",
		"type": "module"
	},
	"icons": {
		"128": "misc/128x128.png"
	}
}
